(no-implicit-core)

;; (include library "core/foundation.neut")
(include library "core/notation.neut")

(define functor-form ((m (hom tau tau)))
  (pi
    ((a tau)
     (b tau)
     (_ (hom a b))
     (_ (m a)))
    (m b)))

(coinductive functor ((f (hom tau tau)))
  (map ((_ (functor f)))
    (functor-form f)))
