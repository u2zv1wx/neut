(include "./base.neut")

(module item
  (definition placeholder unit)

  (definition fact (x)
    (case x
      ((1 1)
       (n (begin
            (let p (core.i32.copy n))
            (case p
              ((pair n1 n2)
               (core.i32.mul n1 (fact (core.i32.sub n2 1)))))))))))

(use item)

(definition id ((a type) (x a)) x)

; (core.i32.print (id i32 1))

(core.i32.print (id i32 (item:fact 10)))
