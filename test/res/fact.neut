;; fact.neut

(include "./base.neut")

(module item

  (reserve some)
  (notation (some e)
    e)

  (definition fact (x)
    (case x
      ((1 (some 1))
       (n (begin
            (let p (core.i32.copy n))
            (case p
              ((pair n1 n2)
               (core.i32.mul n1 (fact (core.i32.sub n2 1))))))))))

  (definition foo unit)

  (definition bar top)
)

(use item)

(core.i32.print (item:fact 10))

;; fact.neut ends here.
