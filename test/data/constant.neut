;; "bypass" the variable binding using `constant`
(let _
  (lambda ((add-i64 tau))
    ((constant add-i64) 1 2)))

;; variable with the same name of a constant
(let _
  (lambda ((i64:add (pi ((_ i64) (_ i64)) i64)))
    (i64:add 1 2)))

